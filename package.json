{
  "name": "@jefferywa/eslint-plugin-import-sort",
  "version": "1.0.2",
  "description": "ESLint plugin for sorting imports by length, alphabetically, and by groups.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "prepare": "yarn build",
    "postinstall": "yarn build && husky"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "import",
    "sort"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jefferywa/eslint-plugin-import-sort.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "24.0.1",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "eslint": "9.28.0",
    "eslint-plugin-prettier": "5.4.1",
    "husky": "9.1.7",
    "jest": "30.0.0",
    "prettier": "3.5.3",
    "ts-jest": "29.4.0",
    "typescript": "5.8.3"
  },
  "author": "Vsevolod Golubinov"
}
